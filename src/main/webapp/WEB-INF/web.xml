<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>it.contrader.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>it.contrader.servlets.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>4000</session-timeout>
  </session-config>
  <servlet>
    <description></description>
    <display-name>UserManagerServlet</display-name>
    <servlet-name>UserManagerServlet</servlet-name>
    <servlet-class>it.contrader.servlets.UserManagerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserManagerServlet</servlet-name>
    <url-pattern>/UserManagerServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>BuildingManagerServlet</display-name>
    <servlet-name>BuildingManagerServlet</servlet-name>
    <servlet-class>it.contrader.servlets.BuildingManagerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BuildingManagerServlet</servlet-name>
    <url-pattern>/BuildingManagerServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AmbienteManagerServlet</display-name>
    <servlet-name>AmbienteManagerServlet</servlet-name>
    <servlet-class>it.contrader.servlets.AmbienteManagerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AmbienteManagerServlet</servlet-name>
    <url-pattern>/AmbienteManagerServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ItemManagerServlet</display-name>
    <servlet-name>ItemManagerServlet</servlet-name>
    <servlet-class>it.contrader.servlets.ItemManagerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ItemManagerServlet</servlet-name>
    <url-pattern>/ItemManagerServlet</url-pattern>
  </servlet-mapping>
</web-app>